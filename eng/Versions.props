    <MicrosoftServiceHubFrameworkVersion>2.6.44</MicrosoftServiceHubFrameworkVersion>
    <MicrosoftVSSDKBuildToolsVersion>16.7.3069</MicrosoftVSSDKBuildToolsVersion>
    <!-- Libs -->
    <SystemCompositionVersion>1.1.0</SystemCompositionVersion>
  </PropertyGroup>
  <PropertyGroup>
    <RestoreSources>
      $(RestoreSources);
      https://dotnet.myget.org/F/roslyn-tools/api/v3/index.json
    </RestoreSources>
  </PropertyGroup>
</Project>
